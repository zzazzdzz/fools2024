function topicStringDecryptor(vv) {
	var r = "";
	var hexTbl = array_create(256, 0);
	// qhipntsgromkuvjl
	hexTbl[ord("q")]=0;hexTbl[ord("h")]=1;hexTbl[ord("i")]=2;hexTbl[ord("p")]=3;
	hexTbl[ord("n")]=4;hexTbl[ord("t")]=5;hexTbl[ord("s")]=6;hexTbl[ord("g")]=7;
	hexTbl[ord("r")]=8;hexTbl[ord("o")]=9;hexTbl[ord("m")]=10;hexTbl[ord("k")]=11;
	hexTbl[ord("u")]=12;hexTbl[ord("v")]=13;hexTbl[ord("j")]=14;hexTbl[ord("l")]=15;
	for (var i=1; i<=string_length(vv); i+=2) {
		var b = hexTbl[string_byte_at(vv, i)] * 0x10;
		b += hexTbl[string_byte_at(vv, i+1)];
		if (b == 0x3d) b = 0x22;
		r += chr(b);
	}
	return r;
}

