function makeStdCol(col){
	if (col == "0") return (c_white);
	if (col == "1") return (make_color_rgb(255,98,98));
	if (col == "2") return (make_color_rgb(108,243,79));
	if (col == "3") return (make_color_rgb(180,180,255));
	if (col == "4") return (make_color_rgb(180,74,180));
	if (col == "5") return (make_color_rgb(255,80,80));
	return "bepis";
}